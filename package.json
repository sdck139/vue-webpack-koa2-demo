{
  "name": "vue-webpack-koa2-demo",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rm -fr dist/$npm_package_name dist/$npm_package_name-$npm_package_version.zip",
    "lint": "eslint app.js backend/*",
    "mkdir": "mkdir -p dist/$npm_package_name/log",
    "copy": "cp -r backend app.js package.json package-lock.json dist/$npm_package_name",
    "postcopy": "cd dist/$npm_package_name && npm i --production",
    "zip": "cd dist && zip -r $npm_package_name-$npm_package_version.zip $npm_package_name",
    "dev": "NODE_ENV=development nodemon --watch app.js --watch backend app.js local",
    "et": "node_modules/.bin/et -c ./frontend/theme/index.css -o ./frontend/styles/",
    "prebuild": "npm run clean && npm run mkdir",
    "build:frontend": "webpack --progress -p --config build/webpack.prod.js",
    "build:backend": "npm run copy",
    "build": "npm run build:frontend && npm run build:backend && npm run zip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdck139/vue-webpack-koa2-demo.git"
  },
  "author": "CK",
  "license": "ISC",
